{
  "manifest_version": 2,
  "name": "GreenLink",
  "version": "1.0",
  "description": "Cute eco-friendly messages when you open a tab!",
  "permissions": [
   "tabs",
   "activeTab"
  ],

  "chrome_url_overrides": {
   "newtab": "popup.html"
  },

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "-"
  }
}
